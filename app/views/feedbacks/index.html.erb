<h3>All your feedback:</h3>

<%if user_signed_in? %>
  <% users_feedback = current_user.feedbacks%>
  <% users_feedback.each do |f| %>
    <% s = Song.find(f.song_id) %>
    <p><%=s.title %>: <%=f.comment %></p>
    <p><%= button_to 'delete', f, method: :delete, class: 'btn btn-danger' %></p>
  <%end%>
<%else%>

<%end%>
