
  <div class="row">
    <div class="col-lg-6 offset-lg-3">
      <p>Follow Andrew on Spotify for updates and new music: </p>
      <iframe width="100%" height="300" scrolling="no" frameborder="no"
      src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/users/167297996&amp;color=%23ff5500&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;show_teaser=true&amp;visual=true"></iframe>
      <% @songs.each do |song| %>
          <p><%=link_to song.title, song.link %></p>
          <p><%=song.iframe.html_safe %></p>
          <% if user_signed_in? %>
            <p>Give feedback here: </p>
            <div class="clearfix">
              <% @feedback = Feedback.new %>
              <%= render 'feedbacks/form', feedback: @feedback, song_id: song.id %>
            </div>
          <br>
          <% end %>
      <% end %>
    </div>
  </div>
